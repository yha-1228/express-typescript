{
  "name": "express-rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write .",
    "start": "npx nodemon -L",
    "tsc": "tsc",
    "ts-node": "ts-node",
    "test": "jest",
    "build:dev": "webpack --config ./config/webpack.dev.js",
    "build:prod": "webpack --config ./config/webpack.prod.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.21.1",
    "express-validator": "^6.12.1",
    "http-errors": "^1.8.1",
    "morgan": "1.10.0",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.16",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintIgnore": [
    "dist",
    "config"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
